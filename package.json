{
  "name": "daily-track-server",
  "version": "0.0.1",
  "description": "track stuff daily",
  "main": "server.js",
  "scripts": {
    "start:dev": "nodemon server.js",
    "build:lessold": "lessc ./src/less/style.less ./dist/css/style.css && copyfiles --flat ./src/css/*.css ./dist/css",
    "build:imgold": "copyfiles --flat ./src/img/* ./dist/img && copyfiles --flat ./src/favicon.ico ./dist",
    "build:bundleold": "browserify ./src/index.js -o ./dist/js/bundle.js && copyfiles --flat ./src/js/*.js ./dist/js",
    "build:cleanold": "rimraf ./dist/css/* ./dist/js/* ./dist/img/*",
    "watch:bundleold": "watchify ./src/index.js -o ./dist/js/bundle.js -v -d",
    "lint": "clear && jshint .",
    "test": "npm run lint && mocha test -u bdd -R spec",
    "build:prod": "webpack --env production",
    "build:dev": "webpack --env development",
    "deploy:heroku": "git push heroku master",
    "server:dev": "npm run test && npm run build:dev && node server.js",
    "server:prod": "npm run test && npm run build:prod && node server.js",
    "build": "npm run build:prod",
    "build:stats": "webpack --env production --profile --json > stats.json",
    "stats": "npm run build:stats && webpack-bundle-analyzer stats.json ./dist",
    "start": "node server.js",
    "deploy": "npm run test && npm run build && npm run deploy:heroku"
  },
  "author": "Kawika Heftel",
  "license": "ISC",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "express-webpack-assets": "^0.1.0",
    "http-errors": "^1.7.2",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-round": "^1.0.1",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "popper.js": "^1.15.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "browserify": "^16.2.3",
    "browserslist": "^4.5.5",
    "clean-webpack-plugin": "^2.0.1",
    "copyfiles": "^2.1.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jshint": "^2.10.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "should": "^13.2.3",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.5.4",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "watchify": "^3.11.1",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "engines": {
    "node": "8.11.1"
  }
}